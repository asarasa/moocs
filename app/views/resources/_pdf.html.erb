<div class="page-header" style="margin-top:0px">
	<h2><%= @resource.name %></h2>
</div>

<object data="<%= @resource.url %>" type="application/pdf" width="100%" height="720">
  alt : <a href="<%= @resource.url %>">Download</a>
</object>

<div class="resource-content">
	<%=raw @resource.content %>
</div>