<% if @test.multianswer %>
 $("#lesson").html("<%= escape_javascript( render 'multi_solved') %>");
<% else %>
 <% if @score.status == "close" %>
   $("#lesson").html("<%= escape_javascript( render 'single_solved') %>");
 <% else %>
   $("#lesson").html("<%= escape_javascript( render 'solve') %>");
 <% end %>
<% end %>