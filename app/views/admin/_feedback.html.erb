<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      <th>Email</th>
      <th>Content</th>
    </tr>
  </thead>

  <tbody>
    <% @feedback.each do |feedback| %>
      <tr>
        <td><%= feedback.date_created.strftime("%d-%m-%Y") %></td>
        <td><%= feedback.name %></td>
        <td><%= feedback.email %></td>
        <td><%= feedback.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>